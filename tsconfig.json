{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "esModuleInterop": true,
        "allowSyntheticDefaultImports": true,
        "strict": true,
        "allowJs": true,
        "checkJs": true,
        "noEmit": true
    },
    "exclude": [
        "node_modules/",
        "tmp/"
    ]
}